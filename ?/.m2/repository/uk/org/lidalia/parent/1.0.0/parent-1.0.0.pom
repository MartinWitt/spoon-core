  <artifactId>parent</artifactId>
  <version>1.0.0</version>
  <packaging>pom</packaging>

  <name>Lidalia Parent POM</name>
  <description>A convenient parent pom defining versions &amp; scopes for common libraries and
    versions &amp; configuration for common plugins via dependencyManagement &amp;
    pluginManagement sections.</description>
  <url>http://projects.lidalia.org.uk/lidalia-parent</url>
  <licenses>
    <license>
      <name>MIT X11 License</name>
      <url>https://github.com/Mahoney/lidalia-parent/raw/master/X11-LICENSE.txt</url>
      <distribution>repo</distribution>
    </license>
  </licenses>

  <scm>
    <url>https://github.com/Mahoney/lidalia-parent</url>
    <connection>scm:git:git@github.com:Mahoney/lidalia-parent.git</connection>
    <developerConnection>scm:git:git@github.com:Mahoney/lidalia-parent.git</developerConnection>
    <tag>parent-1.0.0</tag>
  </scm>

  <modules>
    <module>logging-dependencies</module>
    <module>standard-test-dependencies</module>
    <module>application-logging-dependencies</module>
    <module>opensource-single-module-project</module>
  </modules>

